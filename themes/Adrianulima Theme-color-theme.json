  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#868c8d"
      }
    },
    {
      "scope": "meta.parameters comment.block",
      "settings": {
        "foreground": "#868c8d"
      }
    },
    {
      "scope": [
        "variable.other.constant.object",
        "variable.other.readwrite.alias",
        "meta.import variable.other.readwrite.alias",
        "meta.import variable.other.readwrite",
        "meta.import string",
        "meta.import string.quoted.single punctuation",
        "meta.import string.quoted.double punctuation"
      ],
      "settings": {
        "foreground": "#a0d0f0"
      }
    },
    {
      "scope": [
        "variable.other",
        "variable.other.object",
        "support.type.object"
      ],
      "settings": {
        "foreground": "#e0f0f0"
      }
    },
    {
      "scope": [
        "variable.other.object.property",
        "variable.other.property",
        "support.variable.property"
      ],
      "settings": {
        "foreground": "#e0f0f0"
      }
    },
    {
      "scope": ["entity.name.function", "entity.name.function.method"],
      "settings": {
        "foreground": "#a0d0f0"
      }
    },
    {
      "scope": ["string.unquoted", "meta.object.member"],
      "settings": {
        "foreground": "#e0f0f0"
      }
    },
    {
      "scope": [
        "meta.definition.property entity.name.function",
